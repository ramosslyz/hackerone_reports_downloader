    "summaries": [
        {
            "id": 2062,
            "category": "team",
            "content": "Due to an access control vulnerability it was possible to view the waybill of an arbitrary driver/partner by supplying their UUID to the `/rt/drivers/[DRIVER_PARTNER_UUID]/waybill` endpoint.",
