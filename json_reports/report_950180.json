        {
            "id": 26429,
            "category": "team",
            "content": "@wi11 discovered an issue with Alerts notification emails where supplied HTML tags would be rendered by the email generator. This could allow an attacker to embed arbitrary hyperlinks or images under the header in those Alerts emails.",
            "user": {
                "id": 121713,
                "username": "nr-dm",
                "name": "",
                "bio": "",
                "cleared": false,
                "verified": false,
                "website": null,
                "location": "",
                "created_at": "2016-11-01T17:24:18.402Z",
                "url": "https://hackerone.com/nr-dm",
                "hackerone_triager": false,
                "hackerone_employee": false,
                "user_type": "company",
                "profile_picture_urls": {
                    "small": "/assets/avatars/default-25f7248a18bdf9e2dc8310319b148d66cff430fa0fade6c5f25fee1b8d7f27ed.png",
