    "summaries": [
        {
            "id": 26866,
            "category": "team",
            "content": "The hacker spotted some orphaned references to Uber-branded Node.js library packages and claimed them on the public NPM registry to run their own proof-of-concept code.",
            "user": {
                "id": 548426,
