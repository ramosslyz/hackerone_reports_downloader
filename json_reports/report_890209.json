    "summaries": [
        {
            "id": 206478,
            "category": "team",
            "content": "A vulnerability was found where users with permissions to manage the user section can add others outside of their department by changing the value of \"department_id\" to an empty value. We thank @imran_nisar for reporting this to our team.",
