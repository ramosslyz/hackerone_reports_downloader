        {
            "id": 12284794,
            "is_internal": false,
            "editable": false,
            "type": "Activities::Comment",
            "message": "We looked into your report. This was a known issue and since we are client side wallet, we cannot verify the pin on the server side. We also don't think it is possible to brute force 1m variations without a debugging tool and since require physical connection to the device with developer-level debugging tool. We cannot accept this as a valid vulnerability and we will mark it as informational.",
            "automated_response": false,
            "created_at": "2021-06-29T10:46:13.086Z",
            "updated_at": "2021-06-29T10:46:13.086Z",
            "actor": {
                "username": "mewart",
                "cleared": false,
                "verified": false,
                "url": "/mewart",
